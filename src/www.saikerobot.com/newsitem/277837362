    function textPageCallbackview_text_32_843() {

        $(".absPos").each(function () { $(this).removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '') });

        if ($("#view_text_32_843").parent(".w_tab_container_item ").length > 0) {
            $("#view_text_32_843").parent(".w_tab_container_item ").removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '');
        }
        if ($("#view_text_32_843").parents(".w_tab_container").length > 0) {
            $("#view_text_32_843").parents(".w_tab_container").removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '');
