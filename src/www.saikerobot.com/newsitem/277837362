<script>
    Pagination('view_text_7_843_txt', "首页", "尾页", "上一页", "下一页", textPageCallbackview_text_7_843);

    function textPageCallbackview_text_7_843() {

        $(".absPos").each(function () { $(this).removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '') });

        if ($("#view_text_7_843").parent(".w_tab_container_item ").length > 0) {
            $("#view_text_7_843").parent(".w_tab_container_item ").removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '');
        }
        if ($("#view_text_7_843").parents(".w_tab_container").length > 0) {
            $("#view_text_7_843").parents(".w_tab_container").removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '');
        }
        if ($("#view_text_7_843").parents(".w_tab").length > 0) {
            $("#view_text_7_843").parents(".w_tab").removeAttr("oldbottom").removeAttr("expandh").removeAttr("oheight").removeAttr("cheight").css("min-height", '').css("height", '').css("top", '');
        }

        
        yiburecomputeLayoutHeight();
        initScrollHeight_view_text_7_843();
    }
    function initScrollHeight_view_text_7_843() {
        var currentHeight = $("#view_text_7_843").height();
        var oldHeight = window["view_text_7_843" + "_height"];
        window["view_text_7_843" + "_height"] = currentHeight;
        if (oldHeight != undefined) {
            var subtract = currentHeight - oldHeight;
            if (subtract < 0) {
                
                if ($("#view_text_7_843").parents(".mainSamrtView").length > 0) {
                    $("#view_text_7_843").parents(".mainSamrtView").each(function () {
                        if ($(this).attr("style") != undefined && $(this).attr("style").toLowerCase().indexOf('height') > -1) {
                            var height = $(this).height();
                            var oheight = $(this).attr("oheight");

                            var expandh = $(this).attr("expandh");
                            if (oheight != undefined && expandh != undefined) {
                                oheight = parseInt(oheight);
                                expandh = parseInt(expandh);
                                expandh = expandh + subtract;
                                var newHeight = oheight + expandh;
                                $(this).height(newHeight);
                                $(this).attr("expandh", expandh);
                                if ($(this).children(".yibuFrameContent").length > 0) {
                                    if ($(this).children(".yibuFrameContent").attr("style") != undefined && $(this).children(".yibuFrameContent").attr("style").toLowerCase().indexOf('height') > -1) {
                                        $(this).children(".yibuFrameContent").height(newHeight);
                                    }
                                }
                            }
                        }
                        
                    });
                }
            }
        }
    }
    $(function () {
        window["view_text_7_843" + "_height"] = $("#view_text_7_843").height();
    });
</script>
